{
  "name": "api-patients",
  "version": "0.0.1",
  "description": "An api for ipps patient data",
  "main": "dist",
  "author": "Egor Kirpichev <egor.kirpichev@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "NODE_ENV=development nodemon --watch src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s --copy-files --out-dir dist --presets es2015,stage-0",
    "prestart": "yarn -s build",
    "start": "NODE_ENV=production pm2 start dist",
    "lint": "eslint src && prettier --list-different \"src/**/*.js\"",
    "format": "prettier --write \"src/**/*.js\"",
    "deploy": "yarn run build && now",
    "test": "jest"
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true
  },
  "eslintConfig": {
    "globals": {
      "jest": false,
      "beforeEach": false,
      "afterEach": false,
      "it": false,
      "expect": false,
      "describe": false
    },
    "extends": [
      "airbnb",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_doc"
          ]
        }
      ],
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "mongoose": "^5.3.16",
    "pm2": "^3.2.3",
    "redis": "^2.8.0",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "moxios": "^0.4.0",
    "nodemon": "^1.18.7",
    "prettier": "^1.15.3",
    "supertest": "^3.1.0"
  }
}
